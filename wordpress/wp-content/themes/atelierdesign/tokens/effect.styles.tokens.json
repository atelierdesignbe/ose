{
  "tailwindcss": {
    "shadow-sm": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "2px",
          "spread": "0px",
          "color": "#0000000d"
        }
      ]
    },
    "shadow": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "3px",
          "spread": "0px",
          "color": "#0000000d"
        },
        {
          "offsetX": "0px",
          "offsetY": "1px",
          "blur": "2px",
          "spread": "-1px",
          "color": "#0000001a"
        }
      ]
    },
    "shadow-md": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "4px",
          "blur": "6px",
          "spread": "-1px",
          "color": "#0000001a"
        },
        {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "4px",
          "spread": "-2px",
          "color": "#0000001a"
        }
      ]
    },
    "shadow-lg": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "10px",
          "blur": "15px",
          "spread": "-3px",
          "color": "#0000001a"
        },
        {
          "offsetX": "0px",
          "offsetY": "4px",
          "blur": "6px",
          "spread": "-4px",
          "color": "#0000001a"
        }
      ]
    },
    "shadow-xl": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "20px",
          "blur": "25px",
          "spread": "-5px",
          "color": "#0000001a"
        },
        {
          "offsetX": "0px",
          "offsetY": "8px",
          "blur": "10px",
          "spread": "-6px",
          "color": "#0000001a"
        }
      ]
    },
    "shadow-2xl": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "25px",
          "blur": "50px",
          "spread": "-12px",
          "color": "#00000040"
        }
      ]
    },
    "shadow-inner": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "2px",
          "blur": "4px",
          "spread": "0px",
          "color": "#0000000d",
          "inset": true
        }
      ]
    },
    "shadow-none": {
      "$type": "shadow",
      "$value": [
        {
          "offsetX": "0px",
          "offsetY": "0px",
          "blur": "0px",
          "spread": "0px",
          "color": "#00000000"
        }
      ]
    }
  }
}